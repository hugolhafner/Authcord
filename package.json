{
  "name": "authcord",
  "version": "1.0.0",
  "description": "Middleware to prevent Discord links being leaked.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "pre-commit": "lint-staged",
    "build": "tsc -p tsconfig.json",
    "build:watch": "npm run clean && tsc -w",
    "dev": "NODE_ENV=development npm run start",
    "format": "run-s format-package-json format-imports format-prettier format-eslint",
    "format-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --fix ./ >/dev/null 2>&1 || true",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "format-prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "lint": "run-p lint-eslint",
    "lint--watch": "onchange 'src/**/*' --initial --kill --delay 1000  -- npm run lint",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "typecheck--watch": "npm run typecheck -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unreleased/Authcord.git"
  },
  "keywords": [
    "Authcord",
    "Authentication"
  ],
  "author": "@unreleased",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unreleased/Authcord/issues"
  },
  "homepage": "https://github.com/unreleased/Authcord#readme",
  "dependencies": {
    "@sentry/node": "^5.29.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "hot-shots": "^8.2.1",
    "knex": "^0.21.13",
    "microframework-w3tec": "^0.6.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nocache": "^2.1.0",
    "redis": "^3.0.2",
    "routing-controllers": "^0.9.0-alpha.6",
    "typedi": "^0.8.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.3.5",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.0",
    "typescript": "^4.1.3",
    "typescript-map": "^0.0.7"
  }
}
